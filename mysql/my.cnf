[mysql]

[mysqld]
sql-mode="NO_ZERO_IN_DATE,ERROR_FOR_DIVISION_BY_ZERO,NO_ENGINE_SUBSTITUTION"
default_authentication_plugin = mysql_native_password
character-set-server=utf8
skip-name-resolve

